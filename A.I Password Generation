import openai

api_key = "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"
openai.api_key = api_key

#ChatGPT
def generate_password(prompt, max_tokens=16):
    response = openai.Completion.create(
        model="davinci-002",
        prompt=prompt,
        max_tokens=max_tokens,
        temperature=0.5
    )
    return response.choices[0].text.strip()


prompt = "Generate a strong password with at least 16 characters, including a mix of uppercase letters, lowercase letters, digits, and special characters:"


password = generate_password(prompt)
print("Generated Password:", password)
